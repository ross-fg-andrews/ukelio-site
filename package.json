{
  "name": "ukelio",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "sync-schema": "npx instant-cli@latest push schema --app $(grep VITE_INSTANTDB_APP_ID .env | cut -d '=' -f2) -y",
